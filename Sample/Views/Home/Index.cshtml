@model IEnumerable<Sample.Models.EmployeeData> 

@{
    ViewBag.Title = "Index";
}
@using (Html.BeginForm("ExportToExcel", "Home", FormMethod.Post))
{
    <div>
        <b>Employee Record</b><br  />
        <br  />
        <br  />
        <table>
            <tr>
                <th>Employee Id </th>
                <th>First Name </th>
                <th>Last Name </th>
                <th>Email</th>
                <th>Salary</th>
                <th>Company</th>
                <th>Department</th>
            </tr>
            @foreach (var d in Model)
            {
                <tr>
                    <td>@d.EmployeeId</td>
                    <td>@d.FirstName</td>
                    <td>@d.LastName</td>
                    <td>@d.Email</td>
                    <td>@d.Salary</td>
                    <td>@d.Company</td>
                    <td>@d.Dept</td>

                </tr>   }
        </table>
        <br  /><br  /><br  />
        <div>
            <input type="submit" value="Export" class="btn btn-default" />
        </div>
    </div>
}